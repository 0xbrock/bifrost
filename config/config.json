{
  "client": {
    "drop_excess_requests": false
  },
  "providers": {
    "azure": {
      "keys": [
        {
          "name": "azure-openai-key",
          "value": "env.AZURE_API_KEY",
          "models": ["gpt-4o", "gpt-4o-mini"],
          "weight": 1.0,
          "azure_key_config": {
            "endpoint": "env.AZURE_ENDPOINT",
            "deployments": {
              "gpt-4o": "gpt-4o",
              "gpt-4o-mini": "gpt-4o-mini"
            },
            "api_version": "2024-08-01-preview"
          }
        }
      ],
      "network_config": {
        "default_request_timeout_in_seconds": 120,
        "max_retries": 3,
        "retry_backoff_initial_ms": 500,
        "retry_backoff_max_ms": 5000
      }
    },
    "azure-serverless-llama": {
      "keys": [
        {
          "name": "llama-serverless-key",
          "value": "env.AZURE_SERVERLESS_LLAMA_KEY",
          "models": ["llama-3.3-70b"],
          "weight": 1.0
        }
      ],
      "network_config": {
        "base_url": "env.AZURE_SERVERLESS_LLAMA_ENDPOINT",
        "default_request_timeout_in_seconds": 120,
        "max_retries": 3,
        "retry_backoff_initial_ms": 500,
        "retry_backoff_max_ms": 5000
      },
      "custom_provider_config": {
        "base_provider_type": "openai",
        "allowed_requests": {
          "chat_completion": true,
          "chat_completion_stream": true
        }
      }
    },
    "azure-serverless-mistral": {
      "keys": [
        {
          "name": "mistral-serverless-key",
          "value": "env.AZURE_SERVERLESS_MISTRAL_KEY",
          "models": ["mistral-large"],
          "weight": 1.0
        }
      ],
      "network_config": {
        "base_url": "env.AZURE_SERVERLESS_MISTRAL_ENDPOINT",
        "default_request_timeout_in_seconds": 120,
        "max_retries": 3,
        "retry_backoff_initial_ms": 500,
        "retry_backoff_max_ms": 5000
      },
      "custom_provider_config": {
        "base_provider_type": "openai",
        "allowed_requests": {
          "chat_completion": true,
          "chat_completion_stream": true
        }
      }
    },
    "gemini": {
      "keys": [
        {
          "name": "gemini-key",
          "value": "env.GEMINI_API_KEY",
          "models": ["gemini-2.5-flash", "gemini-2.5-pro", "gemini-2.5-flash-lite", "gemini-2.5-flash-image", "gemini-2.0-flash", "gemini-2.0-flash-lite", "gemini-1.5-flash", "gemini-1.5-pro"],
          "weight": 1.0
        }
      ],
      "network_config": {
        "default_request_timeout_in_seconds": 120,
        "max_retries": 3,
        "retry_backoff_initial_ms": 500,
        "retry_backoff_max_ms": 5000
      }
    },
    "lm-studio": {
      "keys": [
        {
          "name": "lm-studio-key",
          "value": "env.LM_STUDIO_API_KEY",
          "models": ["google/gemma-3-4b", "google/gemma-3-27b", "qwen/qwen3-vl-30b", "qwen/qwen3-coder-30b", "text-embedding-embeddinggemma-300m-qat", "text-embedding-nomic-embed-text-v1.5"],
          "weight": 1.0
        }
      ],
      "network_config": {
        "base_url": "http://host.docker.internal:1234",
        "default_request_timeout_in_seconds": 120,
        "max_retries": 3,
        "retry_backoff_initial_ms": 500,
        "retry_backoff_max_ms": 5000
      },
      "custom_provider_config": {
        "base_provider_type": "openai",
        "allowed_requests": {
          "chat_completion": true,
          "chat_completion_stream": true,
          "embedding": true
        },
        "request_path_overrides": {
          "chat_completion": "/v1/chat/completions",
          "chat_completion_stream": "/v1/chat/completions",
          "embedding": "/v1/embeddings"
        }
      }
    }
  },
  "config_store": {
    "enabled": true,
    "type": "sqlite",
    "config": {
      "path": "./config.db"
    }
  }
}
